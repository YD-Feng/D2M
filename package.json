{
    "name": "D2M",
    "version": "1.0.6",
    "description": "Data to model",
    "license": "ISC",
    "author": {
        "name": "YD-Feng",
        "email": "550284209@qq.com"
    },
    "private": true,
    "scripts": {
        "clear": "rimraf build && rimraf dist",
        "electron": "electron ./src/main",
        "dev": "node start",
        "build": "rimraf build && rimraf dist && webpack -p --progress --hide-modules --env.NODE_ENV=pro",
        "package-win": "npm run build && build --win",
        "package-linux": "npm run build && build --linux",
        "package-mac": "npm run build && build --mac"
    },
    "dependencies": {
        "@antv/g6": "^1.2.8",
        "dot": "^1.1.2",
        "element-ui": "^2.4.9",
        "highlight.js": "^9.13.1",
        "lodash": "^4.17.5",
        "vue": "^2.3.3",
        "vue-router": "^2.5.3",
        "vue2-ace-editor": "0.0.13",
        "vuex": "^2.3.1"
    },
    "devDependencies": {
        "autoprefixer": "^6.6.0",
        "babel-core": "^6.21.0",
        "babel-eslint": "^7.1.1",
        "babel-helper-vue-jsx-merge-props": "^2.0.2",
        "babel-loader": "^6.2.10",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-plugin-transform-vue-jsx": "^3.3.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-stage-0": "^6.22.0",
        "copy-webpack-plugin": "^4.2.0",
        "css-loader": "^0.26.1",
        "electron": "^3.0.12",
        "electron-builder": "^20.38.3",
        "electron-rebuild": "^1.8.2",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.4",
        "html-webpack-plugin": "^3.2.0",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "postcss-loader": "^1.2.1",
        "script-ext-html-webpack-plugin": "^2.1.3",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "vue-loader": "^12.0.3",
        "vue-template-compiler": "^2.3.3",
        "webpack": "^2.5.1",
        "webpack-dev-server": "^2.4.5"
    },
    "main": "build/main.js",
    "build": {
        "productName": "D2M",
        "appId": "com.electron-vue.d2m",
        "artifactName": "${name}-v${version}.${ext}",
        "directories": {
            "buildResources": "build",
            "output": "dist"
        },
        "files": [
            "build/"
        ],
        "npmRebuild": false,
        "asar": true,
        "dmg": {
            "icon": "build/icon/icon.ico",
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "icon": "build/icon/icon.icns"
        },
        "win": {
            "icon": "build/icon/icon.ico",
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        },
        "linux": {
            "icon": "build/icon/icon.png",
            "target": [
                "zip",
                "deb",
                "AppImage"
            ]
        }
    }
}
